{
    "manifest_version": 3,
    "name": "Url Sender",
    "version": "1.0",
    "permissions" :  [
        "tabs", "activeTab", "scripting"    
    ],
    "background" : {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://*/*", "https://*/*"
    ]

}